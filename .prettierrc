{
  "tabWidth": 2,
  "printWidth": 80,
  "singleQuote": false,
  "trailingComma": "es5",
  "semi": true,
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "parser": "babel",
        "trailingComma": "none",
        "semi": true,
        "quoteProps": "as-needed",
        "bracketSpacing": true,
        "arrowParens": "avoid"
      }
    }
  ]
}
